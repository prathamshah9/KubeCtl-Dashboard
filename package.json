    "kubernetes-client": "^9.0.0",
    "lodash": "4.17",
    "node-pty": "^0.10.1",
    "roboto-fontface": "*",
    "semver-compare": "^1.0.0",
    "v-tooltip": "^3.0.0-beta.14",
    "vue": "^2.6.14",
    "vue-router": "^3.0.0",
    "vuetify": "^2.6.10",
    "vuex": "^3.6.2",
